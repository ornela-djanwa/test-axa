{
  "name": "rest-api-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "~14"
  },
  "scripts": {
    "start": "webpack && node -r babel-register index.js",
    "dev": "nodemon",
    "watch": "webpack --watch --mode development",
    "start-api": "json-server ./api.js",
    "delay-api": "json-server ./api.js --delay 10000",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Julien-Decoen/rest-api-test.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Julien-Decoen/rest-api-test/issues"
  },
  "homepage": "https://github.com/Julien-Decoen/rest-api-test#readme",
  "dependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "babel-loader": "^8.2.3",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "css-loader": "^6.7.1",
    "express": "^4.16.4",
    "express-react-views": "^0.11.0",
    "json-server": "^0.17.0",
    "mini-css-extract-plugin": "^2.6.0",
    "node-sass": "^7.0.1",
    "nunjucks": "^3.1.3",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^0.23.1",
    "webpack": "^5.70.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "babel-preset-env": "^1.7.0",
    "nodemon": "^2.0.18",
    "webpack-cli": "^4.9.2"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git/*",
      "node_modules/*",
      ".public/*"
    ],
    "verbose": true,
    "execMap": {
      "js": "node -r babel-register index.js"
    },
    "events": {
      "restart": "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
    },
    "watch": [
      "app/*",
      "front/*"
    ],
    "ext": "js json html css"
  }
}
